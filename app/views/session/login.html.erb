<section id="main">


<h2>Login</h2>

<div class="Sign_Form">
	<%= form_tag '/login', class: 'login-form' do %>
		<p>Username:</br> <%= text_field_tag :username %></p> 
		<p>Password:</br> <%= password_field_tag :login_password %></p> 
		<%= submit_tag("Log In") %>
	<% end %>
</div>
</section>