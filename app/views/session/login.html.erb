<div class="splash">



<div class="Sign_Form">

<h2>Sign In</h2>

	<%= form_tag '/login', class: 'login-form' do %>
		<p>Username:</br> <%= text_field_tag :username %></p> 
		<p>Password:</br> <%= password_field_tag :login_password %> <%= submit_tag("Log In") %> </p> 
	<% end %>
</div>

</div>




