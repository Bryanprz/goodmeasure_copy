<aside id="admin-aside"> 
<header>
<section id="back"><%= link_to 'Back', admin_path %></section>
</header>
</aside>

<section id="main">
 <p class="small-header module"> Editing cohort</p>
<section class="module">
<%= form_for @cohort, url: admin_cohort_path(@cohort) do |f| %>
<%= render partial: 'form', locals: {f: f} %>
<% end %>


<%= link_to " Delete this Cohort", admin_cohort_path(@cohort), class: "fa fa-trash-o", method: :delete, data: { confirm: 'Are you sure?' } %>
</section>
</section>

